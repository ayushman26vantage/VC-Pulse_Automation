{
  "name": "pulsetest",
  "version": "1.0.0",
  "description": "Vantage Pulse Automation Tests with Playwright",
  "main": "Test_D1.js",
  "scripts": {
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "Launch_Reporter": "npm run allure:generate  && npm run allure:open",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:debug": "playwright test --debug",
    "test:chrome": "playwright test --project=chromium",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "install:browsers": "playwright install",
    "test:create_Survey": "playwright test tests/create-survey.spec.js --headed --workers=1 --project='Google Chrome' && npm run Launch_Reporter",
    "test:Dash_overview": "playwright test tests/DashboardOverView.spec.js --headed --workers=1 --project='Google Chrome' && npm run Launch_Reporter"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "vantage-pulse"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.19.21",
    "allure-commandline": "^2.32.2",
    "allure-js-commons": "^3.2.0",
    "allure-playwright": "^3.2.0",
    "playwright": "^1.56.0"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "fast-xml-parser": "^5.0.8",
    "tough-cookie": "^5.1.1",
    "xml2js": "^0.6.2"
  }
}
